<section class="get-in-touch">
   <h1 class="title">Get in touch</h1>
   <form
      action="https://formspree.io/mwkwppdr"
      method="POST"
      id="form-io"
      class="contact-form row"
      >
       <div class="form-field col-lg-6">
         <input id="name" class="input-text js-input" type="text" name="message" required>
         <label class="label" for="name">Name</label>
      </div>
      <div class="form-field col-lg-6 ">
         <input id="email" class="input-text js-input" type="email" name="_replyto" required>
         <label class="label" for="email">E-mail</label>
      </div>
      <div class="form-field col-lg-6 ">
         <input id="company" class="input-text js-input" type="text" name="message" required>
         <label class="label" for="company">Company Name</label>
      </div>
       <div class="form-field col-lg-6 ">
         <input id="phone" class="input-text js-input" type="text" name="message"required>
         <label class="label" for="phone">What do you need?</label>
      </div>
      <div class="form-field col-lg-12">
         <input id="message" class="input-text js-input" type="text" name="message" required>
         <label class="label" for="message">Message (short explenation)</label>
      </div>
      <div class="form-field col-lg-12">
         <input class="submit-btn" type="submit" value="Submit">
      </div>
   </form>
</section>



